{
  "agents": {
    "coder": {
      "maxTokens": 5000,
      "model": "claude-4-opus",
      "reasoningEffort": "medium"
    },
    "task": {
      "maxTokens": 5000,
      "model": "claude-4-opus"
    },
    "title": {
      "maxTokens": 80,
      "model": "claude-4-opus"
    }
  },
  "lsp": {
    "python": {
      "args": [
        "-"
      ],
      "command": "basedpyright",
      "disabled": false
    },
    "rust": {
      "args": [
        "parse",
        "--no-dump"
      ],
      "command": "rust-analyzer",
      "disabled": false
    },
    "typescript": {
      "args": [
        "--stdio"
      ],
      "command": "typescript-language-server",
      "disabled": false
    }
  },
  "shell": {
    "path": "/bin/zsh"
  },
  "tui": {
    "theme": "opencode"
  }
}