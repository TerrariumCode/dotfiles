{
  "lsp": {
    "python": {
      "args": [
        "-"
      ],
      "command": "basedpyright",
      "disabled": false
    },
    "rust": {
      "args": [
        "parse",
        "--no-dump"
      ],
      "command": "rust-analyzer",
      "disabled": false
    },
    "typescript": {
      "args": [
        "--stdio"
      ],
      "command": "typescript-language-server",
      "disabled": false
    }
  },
  "shell": {
    "path": "/bin/zsh"
  },
  "tui": {
    "theme": "opencode"
  }
}